<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true">
    <shape>
      <solid android:color="#FFFFFF"/>
      <corners android:radius="60dp" />
      <stroke android:width="0.5dp" android:color="@color/button_material_dark"/>
      <gradient android:angle="90" android:endColor="@android:color/holo_green_light" 
                android:startColor="@android:color/holo_green_dark" />
    </shape>

  </item>
  <item >
    <shape>
      <corners android:radius="60dp" />
      <stroke android:width="2dp" android:color="@android:color/holo_green_dark"/>
      <solid android:color="@android:color/holo_green_light"/>
    </shape>
  </item>
</selector>